{
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2017,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": [
            1,
            "never"
        ],
        "no-mixed-spaces-and-tabs": [
            1
        ],
        "no-trailing-spaces": [
            1
        ],
        "space-infix-ops": [
            1
        ],
        "quotes": [
            1,
            "single",
            {
                "allowTemplateLiterals": true
            }
        ],
        "no-unused-vars": 0,
        "one-var": [
            1,
            "never"
        ],
        "vars-on-top": 1,
        "no-undef": 0,
        "no-console": 0,
        "no-use-before-define": 1,
        "keyword-spacing": 1,
        "key-spacing": 1,
        "space-before-blocks": 1,
        "space-in-parens": 1,
        "indent": [
            1,
            4
        ],
        "camelcase": [
            1,
            {
                "properties": "always"
            }
        ],
        "comma-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": [
            1,
            "last"
        ],
        "newline-after-var": [
            1,
            "always"
        ],
        "newline-before-return": 1,
        "object-curly-spacing": [
            1,
            "never"
        ],
        "eqeqeq": 1,
        "eol-last": [
            1,
            "never"
        ],
        "max-len": [1, 120, 4, {
            "ignoreComments": true
        }]
    }
}